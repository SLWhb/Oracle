  CREATE OR REPLACE PACKAGE "AFS"."AFS_FILTER" AS 

  --Функция проверки входящей заявки на соответсвие критерям фильтрации.
  ----Входной параметр v_RID - ID заявки
  FUNCTION CHECK_REQUEST_ID_MAIN (v_RID IN NUMBER) RETURN NUMBER ;
  
  --Функция проверки найденной заявки на соответсвие критерям фильтрации.
  ----Входной параметр v_RID - ID найденной по фроду заявки
  FUNCTION CHECK_REQUEST_ID_REL (v_RID IN NUMBER) RETURN NUMBER ;

END AFS_FILTER;

/
--------------------------------------------------------
--  DDL for Package AFS_RULES_ADR
--------------------------------------------------------

  CREATE OR REPLACE PACKAGE "AFS"."AFS_RULES_ADR" AS 
-- ========================================================================
-- ==    ПАКЕТ со списком фрод правил. Сравнение по адресу"
-- ========================================================================
-- ==	СОЗДАНИЕ:		  06.2016 (ТРАХАЧЕВ В.В.)
-- ==	МОДИФИКАЦИЯ:	06.2016 (ТРАХАЧЕВ В.В.)
-- ========================================================================
  /*Описание параметров для фрод правила.
        v_Request_ID IN NUMBER    - !входной параметр REQUEST_ID заявки из ПКК
        v_Person_ID IN NUMBER     - входной параметр PERSON_ID заявки из ПКК. 
        v_OutCode IN VARCHAR2     - Код внешней системы (для синхронизации между системами)
        v_InArray IN VARCHAR2     - входной параметр для расширения (массив данных)
 
        v_OutNumber OUT NUMBER    - !Выходные данные (результат по фрод-правилу)
                                      0   - фрода нет
                                      1   - фрод есть
                                      -1  - ошибка
                                      9  - нет информации. Т.к. будут использоваться данные ПКК, то считаю код неактуальным
        v_OutChar IN OUT VARCHAR2 - строковый эквивалент возвращаемого значения
        v_OutArray OUT VARCHAR2   - выходной параметр для расширения (массив данных)
        v_ErrorCode OUT NUMBER    - Код ошибки (внутренний код ошибки объекта, если более Нуля то значит возникла проблема при работе)
        v_ErrorText OUT VARCHAR2  - Описание ошибки (детальное описание ошибки)
  */

  PROCEDURE FRAUD_0001(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );
                        
  PROCEDURE FRAUD_0002(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );
                        
  PROCEDURE FRAUD_0003(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );
                        
  PROCEDURE FRAUD_0004(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );

  PROCEDURE FRAUD_0005(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );
  PROCEDURE FRAUD_0006(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        );
                             
END AFS_RULES_ADR;

/
--------------------------------------------------------
--  DDL for Package Body AFS_RULES_ADR
--------------------------------------------------------

  CREATE OR REPLACE PACKAGE BODY "AFS"."AFS_RULES_ADR" AS

  PROCEDURE FRAUD_0001(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      --pragma autonomous_transaction;  
  BEGIN 
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END; 
      
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT DISTINCT * FROM 
                (SELECT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                  ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                  ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                  --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                  ,'Адр.прж:'||SRC.regions_names||', '||SRC.areas_names||', '||SRC.cities_names||', '||SRC.streets_names
                      ||', '||SRC.house||', '||SRC.build||', '||SRC.flat as INFO_EQUAL
                  ,'Фио+Др.:'||SRC.FIO||' '||SRC.BIRTH as INFO_NOT_EQUAL
                  ,'Фио+Др.:'||REL.FIO||' '||REL.BIRTH as INFO_NOT_EQUAL_REL
                  /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                              ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK*/
                  ,SRC.FIO as FIO, SRC.BIRTH as DR
                  ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                  ,1 AS FRAUD_OUTNUMBER
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                        ORDER BY src.CREATED_DATE DESC, src.ADDRESS_AKT DESC, COALESCE(src.ADDRESS_MOD, src.ADDRESS_CREATED) DESC
                          ) as RNK
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,rel.REQUEST_ID,rel.OBJECTS_ID 
                        ORDER BY rel.CREATED_DATE DESC, rel.ADDRESS_AKT DESC, COALESCE(rel.ADDRESS_MOD, rel.ADDRESS_CREATED) DESC
                          ) as RNK_REL
                  --,REL.ADDRESS_MOD, REL.ADDRESS_CREATED
                  --,SRC.ADDRESS_MOD as AM, SRC.ADDRESS_CREATED as AC
                FROM AFS.VR_FMG_FB SRC
                INNER JOIN AFS.VR_FMG_FB REL --таблица поиска связей
                  ON --RULE: определяем фрод-правило поиска 
                    rel.ADDRESS_ID=src.ADDRESS_ID and
                    REL.regions_names=SRC.regions_names 
                      AND REL.areas_names=SRC.areas_names
                      AND REL.cities_names=SRC.cities_names
                      AND REL.streets_names=SRC.streets_names
                      AND REL.house=SRC.house
                      AND REL.build=COALESCE(SRC.build,' ')
                      AND REL.flat=COALESCE(SRC.flat,' ')
                      AND NOT SRC.house=' ' AND NOT SRC.house IS NULL
                      AND REL.ADDRESS_AKT=1
                      AND REL.FIO ^= SRC.FIO --AND SRC.DR ^= REL.DR
                      AND REL.CREATED_DATE BETWEEN SRC.CREATED_DATE-91*2 AND SRC.CREATED_DATE
                      AND REL.REQUEST_ID < SRC.REQUEST_ID
                WHERE SRC.REQUEST_ID = v_Request_ID -- IN (61159401,61160160,61159965) 
                  AND SRC.ADDRESS_AKT=1
                  --EXCEPT: определяем доп. исключения по фрод-правилу (исключение пустых, разные физики и т.п.)
                  AND NOT SRC.regions_names='-' AND NOT (SRC.cities_names='-' and SRC.streets_names='-')
                  --AND NOT SRC.LA_APARTMENT='-' AND NOT SRC.LA_APARTMENT IS NULL
                  AND NOT SRC.FIO IS NULL AND NOT REL.FIO IS NULL AND NOT SRC.FIO='-' AND NOT REL.FIO='-'
                  AND NOT SRC.BIRTH IS NULL AND NOT REL.BIRTH IS NULL AND NOT SRC.BIRTH='-' AND NOT REL.BIRTH='-'
                  AND NOT SRC.OBJECTS_ID=REL.OBJECTS_ID
                  --проверка адреса на в/ч
                  /*AND NOT EXISTS(SELECT * FROM SFF.APPLICATIONS WHERE REQUEST_ID=SRC.REQUEST_ID
                                  AND regions_names=SRC.regions_names
                                  AND areas_names=SRC.areas_names
                                  AND cities_names=SRC.cities_names
                                  AND streets_names=SRC.streets_names
                                  AND house=SRC.house
                                  AND NVL(build, '-')=NVL(SRC.build, '-')
                                  AND NVL(flat, '-')=NVL(SRC.flat, '-')
                                  AND (WORK_ORG_NAME LIKE '%В/Ч%' OR WORK_ORG_NAME LIKE 'ВЧ%') )*/
                                  ) TAB 
              WHERE TAB.RNK = 1 and TAB.RNK_REL = 1
                -- исключение однофамильцев 
                AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
                -- исключаем девичьи фамилии
                AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL )
                --PostCheck: Проверка на родственников
                /*AND NOT EXISTS(SELECT OBJECTS_ID FROM CPD.FAMILY@DBLINK_PKK fam
                            WHERE fam.OBJECTS_ID=TAB.PERSON_ID AND fam.OB_ID=TAB.PERSON_ID_REL
                               AND fam.OBJECTS_TYPE=2 AND fam.OB_TYPE=2)*/
                --PostCheck: Модифицированная проверка на родственников
                AND NOT AFS.FN_IS_FAMILY_REL(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1 
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID_REL, TAB.PERSON_ID)=1
                --оставляем только заявки не попавшие под общий фильтр
                AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
                )
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      --COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0001;

  PROCEDURE FRAUD_0002(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      --pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
      
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT DISTINCT * FROM 
                (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                  ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                  ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                  --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                  ,'Адр.прж:'||SRC.regions_names||', '||SRC.areas_names||', '||SRC.cities_names||', '||SRC.streets_names
                      ||', '||SRC.house||', '||SRC.build||', '||SRC.flat as INFO_EQUAL
                  ,'Паспорт:'||doc_src.DOCUMENTS_SERIAL||' '||doc_src.DOCUMENTS_NUMBER as INFO_NOT_EQUAL
                  ,'Паспорт:'||doc_rel.DOCUMENTS_SERIAL||' '||doc_rel.DOCUMENTS_NUMBER as INFO_NOT_EQUAL_REL
                  /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                              ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK*/
                  ,SRC.FIO as FIO, SRC.BIRTH as DR
                  ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                  ,1 AS FRAUD_OUTNUMBER
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                        ORDER BY src.CREATED_DATE DESC, src.ADDRESS_AKT DESC, COALESCE(src.ADDRESS_MOD, src.ADDRESS_CREATED) DESC
                          ) as RNK
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,rel.REQUEST_ID,rel.OBJECTS_ID 
                        ORDER BY rel.CREATED_DATE DESC, rel.ADDRESS_AKT DESC, COALESCE(rel.ADDRESS_MOD, rel.ADDRESS_CREATED) DESC
                          ) as RNK_REL
                  --,REL.ADDRESS_MOD, REL.ADDRESS_CREATED
                  --,SRC.ADDRESS_MOD as AM, SRC.ADDRESS_CREATED as AC
                FROM AFS.VR_FMG_FB SRC
                INNER JOIN AFS.VR_FMG_FB REL --таблица поиска связей
                  ON --RULE: определяем фрод-правило поиска 
                    rel.ADDRESS_ID=src.ADDRESS_ID and
                    REL.regions_names=SRC.regions_names 
                      AND REL.areas_names=SRC.areas_names
                      AND REL.cities_names=SRC.cities_names
                      AND REL.streets_names=SRC.streets_names
                      AND REL.house=SRC.house
                      AND REL.build=COALESCE(SRC.build,' ')
                      AND REL.flat=COALESCE(SRC.flat,' ')
                      AND NOT SRC.house=' ' AND NOT SRC.house IS NULL
                      AND REL.ADDRESS_AKT=1
                      AND REL.CREATED_DATE BETWEEN SRC.CREATED_DATE-91*2 AND SRC.CREATED_DATE
                      AND REL.REQUEST_ID < SRC.REQUEST_ID
                      AND NOT SRC.regions_names='-' AND NOT (SRC.cities_names='-' and SRC.streets_names='-')
                INNER JOIN AFS.VIEW_DOCUMENTS_HISTORY doc_src
                    ON SRC.OBJECTS_ID=doc_src.OBJECTS_ID AND doc_src.DOCUMENTS_TYPE=21 and doc_src.DOCUMENTS_AKT=1 
                      AND SRC.CREATED_DATE+1>doc_src.DOCUMENTS_CREATED
                      AND SRC.CREATED_DATE+1>TO_DATE(doc_src.DOCUMENTS_DATE, 'dd.mm.yyyy')
                INNER JOIN AFS.VIEW_DOCUMENTS_HISTORY doc_rel
                    ON REL.OBJECTS_ID=doc_rel.OBJECTS_ID AND doc_rel.DOCUMENTS_TYPE=21 and doc_rel.DOCUMENTS_AKT=1
                      AND REL.CREATED_DATE+1>doc_rel.DOCUMENTS_CREATED
                      --AND REL.CREATED_DATE+1>TO_DATE(doc_rel.DOCUMENTS_DATE, 'dd.mm.yyyy')
                WHERE SRC.REQUEST_ID = v_Request_ID --IN (61691446,61690794 --,61690096,61689618,61689454,61689446) 
                  AND SRC.ADDRESS_AKT=1
                  AND doc_rel.DOCUMENTS_SERIAL ^= doc_src.DOCUMENTS_SERIAL
                  AND doc_rel.DOCUMENTS_NUMBER ^= doc_src.DOCUMENTS_NUMBER
                  --EXCEPT: определяем доп. исключения по фрод-правилу (исключение пустых, разные физики и т.п.)
                  AND NOT doc_src.DOCUMENTS_SERIAL IS NULL AND NOT doc_rel.DOCUMENTS_SERIAL IS NULL 
                  AND NOT doc_src.DOCUMENTS_SERIAL='-' AND NOT doc_rel.DOCUMENTS_SERIAL='-'
                  AND NOT doc_src.DOCUMENTS_NUMBER IS NULL AND NOT doc_rel.DOCUMENTS_NUMBER IS NULL 
                  AND NOT doc_src.DOCUMENTS_NUMBER='-' AND NOT doc_rel.DOCUMENTS_NUMBER='-'
                  AND NOT SRC.OBJECTS_ID=REL.OBJECTS_ID
                  --проверка адреса на в/ч
                  /*AND NOT EXISTS(SELECT * FROM SFF.APPLICATIONS WHERE REQUEST_ID=SRC.REQUEST_ID
                                  AND regions_names=SRC.regions_names
                                  AND areas_names=SRC.areas_names
                                  AND cities_names=SRC.cities_names
                                  AND streets_names=SRC.streets_names
                                  AND house=SRC.house
                                  AND NVL(build, '-')=NVL(SRC.build, '-')
                                  AND NVL(flat, '-')=NVL(SRC.flat, '-')
                                  AND (WORK_ORG_NAME LIKE '%В/Ч%' OR WORK_ORG_NAME LIKE 'ВЧ%') )*/
                                  ) TAB 
              WHERE TAB.RNK=1 and tab.RNK_REL=1
                -- исключение однофамильцев 
                AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
                -- исключаем девичьи фамилии
                AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL )
               --PostCheck: Модифицированная проверка на родственников
                AND NOT AFS.FN_IS_FAMILY_REL(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1 
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID_REL, TAB.PERSON_ID)=1
                --оставляем только заявки не попавшие под общий фильтр
                AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
        )
              --оставляем только заявки не попавшие под общий фильтр
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      --COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0002;

  PROCEDURE FRAUD_0003(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      --pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
      
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT DISTINCT * FROM 
                (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                  ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                  ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                  --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                  ,'Адр.прж:'||SRC.regions_names||', '||SRC.areas_names||', '||SRC.cities_names||', '||SRC.streets_names
                      ||', '||SRC.house||', '||SRC.build||', '||SRC.flat as INFO_EQUAL
                  ,'Тел.моб:'||ph_src.PHONE as INFO_NOT_EQUAL
                  ,'Тел.моб:'||ph_rel.PHONE as INFO_NOT_EQUAL_REL
                  /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                              ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK*/
                  ,SRC.FIO as FIO, SRC.BIRTH as DR
                  ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                  ,1 AS FRAUD_OUTNUMBER
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                        ORDER BY src.CREATED_DATE DESC, src.ADDRESS_AKT DESC, COALESCE(src.ADDRESS_MOD, src.ADDRESS_CREATED) DESC
                          ) as RNK
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,rel.REQUEST_ID,rel.OBJECTS_ID 
                        ORDER BY rel.CREATED_DATE DESC, rel.ADDRESS_AKT DESC, COALESCE(rel.ADDRESS_MOD, rel.ADDRESS_CREATED) DESC
                          ) as RNK_REL
                  --,REL.ADDRESS_MOD, REL.ADDRESS_CREATED
                  --,SRC.ADDRESS_MOD as AM, SRC.ADDRESS_CREATED as AC
                FROM AFS.VR_FMG_FB src
                INNER JOIN AFS.VR_FMG_FB rel --таблица поиска связей
                  ON --RULE: определяем фрод-правило поиска 
                    rel.ADDRESS_ID=src.ADDRESS_ID and
                    REL.regions_names=SRC.regions_names 
                      AND REL.areas_names=SRC.areas_names
                      AND REL.cities_names=SRC.cities_names
                      AND REL.streets_names=SRC.streets_names
                      AND REL.house=SRC.house
                      AND REL.build=COALESCE(SRC.build,' ')
                      AND REL.flat=COALESCE(SRC.flat,' ')
                      AND NOT SRC.house=' ' AND NOT SRC.house IS NULL
                      AND REL.ADDRESS_AKT=1
                      AND REL.CREATED_DATE BETWEEN SRC.CREATED_DATE-91*2 AND SRC.CREATED_DATE
                      AND REL.REQUEST_ID < SRC.REQUEST_ID
                      AND NOT SRC.regions_names='-' AND NOT (SRC.cities_names='-' and SRC.streets_names='-')
                INNER JOIN CPD.PHONES ph_src
                    ON ph_src.OBJECTS_ID=src.OBJECTS_ID AND ph_src.OBJECTS_TYPE=2 AND src.CREATED_DATE+10>ph_src.phones_created
                      and ph_src.PHONES_AKT=1
                INNER JOIN CPD.PHONES ph_rel
                    ON ph_rel.OBJECTS_ID=rel.OBJECTS_ID AND ph_rel.OBJECTS_TYPE=2 AND rel.CREATED_DATE+10>ph_rel.phones_created
                      --and ph_rel.PHONES_AKT=1
                WHERE src.REQUEST_ID = v_Request_ID  --IN (62784235,62783599 --,62783182,62781772,62781258) 
                  AND SRC.ADDRESS_AKT=1
                  AND ph_rel.PHONE ^= ph_src.PHONE
                  --EXCEPT: определяем доп. исключения по фрод-правилу (исключение пустых, разные физики и т.п.)
                  AND NOT ph_src.PHONE IS NULL AND NOT ph_rel.PHONE IS NULL
                  AND NOT ph_src.PHONE LIKE '%000000%' AND NOT ph_rel.PHONE LIKE '%000000%'
                  AND NOT ph_src.PHONE LIKE '%999999%' AND NOT ph_rel.PHONE LIKE '%999999%'
                  AND NOT src.OBJECTS_ID=rel.OBJECTS_ID
                  --проверка адреса на в/ч
                  /*AND NOT EXISTS(SELECT * FROM SFF.APPLICATIONS WHERE REQUEST_ID=SRC.REQUEST_ID
                                  AND regions_names=SRC.regions_names
                                  AND areas_names=SRC.areas_names
                                  AND cities_names=SRC.cities_names
                                  AND streets_names=SRC.streets_names
                                  AND house=SRC.house
                                  AND NVL(build, '-')=NVL(SRC.build, '-')
                                  AND NVL(flat, '-')=NVL(SRC.flat, '-')
                                  AND (WORK_ORG_NAME LIKE '%В/Ч%' OR WORK_ORG_NAME LIKE 'ВЧ%') )*/
                                  ) TAB 
              WHERE TAB.RNK=1 and TAB.RNK_REL=1 
                -- исключение однофамильцев 
                AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
                -- исключаем девичьи фамилии
                AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL )
               --PostCheck: Модифицированная проверка на родственников
                AND NOT AFS.FN_IS_FAMILY_REL(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1 
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID_REL, TAB.PERSON_ID)=1
                --оставляем только заявки не попавшие под общий фильтр
                AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
        )
              --оставляем только заявки не попавшие под общий фильтр
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      --COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0003;

  PROCEDURE FRAUD_0004(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      --pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
      
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT DISTINCT * FROM 
                (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                  ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                  ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                  --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                  ,'Адр.прж:'||SRC.regions_names||', '||SRC.areas_names||', '||SRC.cities_names||', '||SRC.streets_names
                      ||', '||SRC.house||', '||SRC.build||', '||SRC.flat as INFO_EQUAL
                  ,'Тел.дом:'||TO_CHAR(src.PHONE_FMG) as INFO_NOT_EQUAL
                  ,'Тел.дом:'||TO_CHAR(rel.PHONE_FMG) as INFO_NOT_EQUAL_REL
                  /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                              ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK*/
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                        ORDER BY src.CREATED_DATE DESC, src.ADDRESS_AKT DESC, COALESCE(src.ADDRESS_MOD, src.ADDRESS_CREATED) DESC
                          , src.PHONES_AKT DESC, COALESCE(src.PHONES_MOD, src.PHONES_CREATED) DESC
                          ) as RNK
                  ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,rel.REQUEST_ID,rel.OBJECTS_ID 
                        ORDER BY rel.CREATED_DATE DESC, rel.ADDRESS_AKT DESC, COALESCE(rel.ADDRESS_MOD, rel.ADDRESS_CREATED) DESC
                          , rel.PHONES_AKT DESC, COALESCE(rel.PHONES_MOD, rel.PHONES_CREATED) DESC
                          ) as RNK_REL
                  ,SRC.FIO as FIO, SRC.BIRTH as DR
                  ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                  ,1 AS FRAUD_OUTNUMBER
                  --,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                              --ORDER BY REL.REQUEST_ID DESC, COALESCE(REL.ADDRESS_MOD, REL.ADDRESS_CREATED) DESC) as R_N_APP
                  --,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID
                              --ORDER BY SRC.REQUEST_ID DESC, COALESCE(SRC.ADDRESS_MOD, SRC.ADDRESS_CREATED) DESC) as R_N_SRC
                  --,REL.ADDRESS_MOD, REL.ADDRESS_CREATED
                  --,SRC.ADDRESS_MOD as AM, SRC.ADDRESS_CREATED as AC
                FROM AFS.VR_FMG_FB_PH src
                INNER JOIN AFS.VR_FMG_FB_PH rel --таблица поиска связей
                  ON --RULE: определяем фрод-правило поиска 
                    rel.ADDRESS_ID=src.ADDRESS_ID and
                    SRC.regions_names=SRC.regions_names 
                      AND REL.areas_names=SRC.areas_names
                      AND REL.cities_names=SRC.cities_names
                      AND REL.streets_names=SRC.streets_names
                      AND REL.house=SRC.house
                      AND REL.build=COALESCE(SRC.build,' ')
                      AND REL.flat=COALESCE(SRC.flat,' ')
                      AND NOT SRC.house=' ' AND NOT SRC.house IS NULL
                      AND REL.ADDRESS_AKT=1
                      AND REL.CREATED_DATE BETWEEN SRC.CREATED_DATE-91*2 AND SRC.CREATED_DATE
                      AND REL.REQUEST_ID < SRC.REQUEST_ID
                      AND NOT SRC.regions_names='-' AND NOT (SRC.cities_names='-' and SRC.streets_names='-')
                WHERE src.REQUEST_ID = v_Request_Id --IN (60595238,60534626 ,60521163,60505510,60433500 )     
                  AND SRC.ADDRESS_AKT=1
                  AND src.PHONE_FMG ^= rel.PHONE_FMG
                  --EXCEPT: определяем доп. исключения по фрод-правилу (исключение пустых, разные физики и т.п.)
                  AND NOT src.PHONE_FMG IS NULL AND NOT rel.PHONE_FMG IS NULL
                  AND NOT src.PHONE_FMG LIKE '%000000%' AND NOT rel.PHONE_FMG LIKE '%000000%'
                  AND NOT src.PHONE_FMG LIKE '%999999%' AND NOT rel.PHONE_FMG LIKE '%999999%'
                  AND NOT src.PHONE_FMG = '-' AND NOT rel.PHONE_FMG = '-'
                  AND NOT src.OBJECTS_ID=rel.OBJECTS_ID
                  --проверка адреса на в/ч
                  /*AND NOT EXISTS(SELECT * FROM SFF.APPLICATIONS WHERE REQUEST_ID=SRC.REQUEST_ID
                                  AND regions_names=SRC.regions_names
                                  AND areas_names=SRC.areas_names
                                  AND cities_names=SRC.cities_names
                                  AND streets_names=SRC.streets_names
                                  AND house=SRC.house
                                  AND NVL(build, '-')=NVL(SRC.build, '-')
                                  AND NVL(flat, '-')=NVL(SRC.flat, '-')
                                  AND (WORK_ORG_NAME LIKE '%В/Ч%' OR WORK_ORG_NAME LIKE 'ВЧ%') )*/
                                  ) tab
              WHERE tab.RNK=1 and tab.RNK_REL=1
                -- исключение однофамильцев 
                AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
                -- исключаем девичьи фамилии
                AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL )
               --PostCheck: Модифицированная проверка на родственников
                AND NOT AFS.FN_IS_FAMILY_REL(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1 
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID_REL, TAB.PERSON_ID)=1
                --оставляем только заявки не попавшие под общий фильтр
                AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
        )
              --оставляем только заявки не попавшие под общий фильтр
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      --COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0004;

  PROCEDURE FRAUD_0005(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0005;
  
  PROCEDURE FRAUD_0006(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0006;
END AFS_RULES_ADR;

