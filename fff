create table afs.GPS_AREA_LOCK
(
ID_AREA_GPS NUMBER,
AREA_GPS_TYPE NUMBER,
AREA_GPS_X NUMBER,
AREA_GPS_Y NUMBER,
AREA_GPS_RADIUS NUMBER ,
AREA_GPS_AKT NUMBER ,
AREA_GPS_NAME VARCHAR2(1000) ,
AREA_GPS_ADDRESS_STR VARCHAR2(2000),
AREA_GPS_DECISION_TYPE NUMBER ,
AREA_GPS_COMMENT VARCHAR2(2000) ,
AREA_GPS_REGION_NAME VARCHAR2(254) 
);


COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_TYPE" IS 'Тип зоны';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_X" IS 'Координата зоны X';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_Y" IS 'Координата зоны Y';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_RADIUS" IS 'Радиус зоны в метрах';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_AKT" IS 'Актуальность зоны (0-неактуально, 1 - актуально)';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_NAME" IS 'Название объекта';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_ADDRESS_STR" IS 'Адрес строкой';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_DECISION_TYPE" IS 'Тип решения (0-нейтрально, 1- отказ, 2- положительная зона)';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_COMMENT" IS 'Комментарий';
COMMENT ON COLUMN "AFS"."GPS_AREA_LOCK"."AREA_GPS_REGION_NAME" IS 'Регион';
COMMENT ON TABLE "AFS"."GPS_AREA_LOCK"  IS 'Таблица с данными о запрещенных зонах при выдаче с планшетов';



