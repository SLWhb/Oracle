  CREATE OR REPLACE VIEW "AFS"."VR_FMG_PH" ("REQUEST_ID", "OBJECTS_ID", "CREATED_DATE", "MODIFICATION_DATE", "CREATED_GROUP_ID", "STATUS_ID", "SCORE_TREE_ROUTE_ID", "TYPE_REQUEST_ID", "ADDRESS_HISTORY_ID", "OBJECTS_TYPE", "ADDRESS_AKT", "ADDRESS_CREATED", "ADDRESS_MOD", "ADDRESS_STR", "REGIONS_NAMES", "AREAS_NAMES", "CITIES_NAMES", "STREETS_NAMES", "HOUSE", "BUILD", "FLAT", "ADDRESS_ID", "OBJECTS_TYPE_PH", "PHONE_FMG", "PHONES_AKT", "PHONES_CREATED", "PHONES_MOD", "PHONES_ID") AS 
  SELECT cr.REQUEST_ID
    ,cr.OBJECTS_ID
    ,cr.CREATED_DATE
    ,cr.MODIFICATION_DATE
    ,cr.CREATED_GROUP_ID
,cr.STATUS_ID
,cr.SCORE_TREE_ROUTE_ID
,cr.TYPE_REQUEST_ID
    ,vah_FMG.ADDRESS_HISTORY_ID
    ,vah_FMG.OBJECTS_TYPE
    ,vah_FMG.ADDRESS_AKT
    ,vah_FMG.ADDRESS_CREATED
    ,vah_FMG.MODIFICATION_DATE AS ADDRESS_MOD
    ,vah_FMG.ADDRESS_STR 
    ,vah_FMG.regions_names
    ,vah_FMG.areas_names
    ,vah_FMG.cities_names
    ,vah_FMG.streets_names
    ,vah_FMG.house 
    ,vah_FMG.build
    ,vah_FMG.flat
    ,vah_FMG.ADDRESS_ID
/*    ,COALESCE(TO_CHAR((SELECT LAST_VALUE(PHONE) OVER(PARTITION BY ph_fmg.OBJECTS_ID   --по каждому адресу последний телефон
                ORDER BY ph_fmg.PHONES_AKT DESC, COALESCE(ph_fmg.MODIFICATION_DATE, SYSDATE) DESC, ph_fmg.PHONES_CREATED DESC) RANK
            FROM CPD.PHONES ph_fmg 
            WHERE OBJECTS_ID=vah_FMG.ADDRESS_ID and cr.CREATED_DATE+1 > vah_FMG.address_created
              AND ph_fmg.OBJECTS_TYPE=8 AND cr.CREATED_DATE+1>ph_fmg.phones_created AND ROWNUM=1))
        ,'-') AS PHONE_FMG*/
    ,ph_pmg.OBJECTS_TYPE as OBJECTS_TYPE_PH
    ,ph_pmg.PHONE as PHONE_FMG
    ,ph_pmg.PHONES_AKT
    ,ph_pmg.PHONES_CREATED
    ,ph_pmg.MODIFICATION_DATE AS PHONES_MOD
    ,ph_pmg.PHONES_ID
  FROM KREDIT.C_REQUEST cr
  INNER JOIN AFS.VIEW_ADDRESS_HISTORY vah_FMG
    ON vah_FMG.OBJECTS_ID=cr.OBJECTS_ID AND vah_FMG.OBJECTS_TYPE=2 AND cr.CREATED_DATE+10>vah_FMG.address_created
  INNER JOIN CPD.PHONES ph_pmg 
    ON ph_pmg.OBJECTS_ID=vah_FMG.ADDRESS_ID and cr.CREATED_DATE+1 > vah_FMG.address_created
              AND ph_pmg.OBJECTS_TYPE=8 AND cr.CREATED_DATE+1>ph_pmg.phones_created
               AND cr.CREATED_DATE+1>ph_pmg.MODIFICATION_DATE;

   COMMENT ON TABLE "AFS"."VR_FMG_PH"  IS 'Вьюха с привязкой к заявке данных по ФМЖ + Тел. фмж';
--------------------------------------------------------
--  DDL for View VR_MOB
--------------------------------------------------------

  CREATE OR REPLACE VIEW "AFS"."VR_MOB" ("REQUEST_ID", "OBJECTS_ID", "CREATED_DATE", "MODIFICATION_DATE", "CREATED_GROUP_ID", "STATUS_ID", "SCORE_TREE_ROUTE_ID", "TYPE_REQUEST_ID", "OBJECTS_TYPE", "PHONE_MOB", "PHONES_AKT", "PHONES_CREATED", "PHONES_MOD") AS 
  SELECT cr.REQUEST_ID
    ,cr.OBJECTS_ID
    ,cr.CREATED_DATE
    ,cr.MODIFICATION_DATE
    ,cr.CREATED_GROUP_ID
	,cr.STATUS_ID
	,cr.SCORE_TREE_ROUTE_ID
	,cr.TYPE_REQUEST_ID
    ,ph_mob.OBJECTS_TYPE
    ,ph_mob.PHONE AS PHONE_MOB
    ,ph_mob.PHONES_AKT
    ,ph_mob.PHONES_CREATED
    ,ph_mob.MODIFICATION_DATE AS PHONES_MOD
  FROM CPD.PHONES ph_mob, KREDIT.C_REQUEST cr
  WHERE ph_mob.OBJECTS_ID=cr.OBJECTS_ID AND ph_mob.OBJECTS_TYPE=2 AND cr.CREATED_DATE+10>ph_mob.phones_created;

   COMMENT ON TABLE "AFS"."VR_MOB"  IS 'Вьюха с привязкой к заявке Мобильного телефона';
--------------------------------------------------------
--  DDL for View VR_MOB_FB
--------------------------------------------------------

  CREATE OR REPLACE VIEW "AFS"."VR_MOB_FB" ("REQUEST_ID", "OBJECTS_ID", "CREATED_DATE", "MODIFICATION_DATE", "CREATED_GROUP_ID", "STATUS_ID", "SCORE_TREE_ROUTE_ID", "TYPE_REQUEST_ID", "FIO", "FIO_AKT", "FIO_CREATED", "FIO_MOD", "BIRTH", "BIRTH_AKT", "BIRTH_CREATED", "BIRTH_MOD", "OBJECTS_TYPE", "PHONE_MOB", "PHONES_AKT", "PHONES_CREATED", "PHONES_MOD") AS 
  SELECT cr.REQUEST_ID
    ,cr.OBJECTS_ID
    ,cr.CREATED_DATE
    ,cr.MODIFICATION_DATE
    ,cr.CREATED_GROUP_ID
	,cr.STATUS_ID
	,cr.SCORE_TREE_ROUTE_ID
	,cr.TYPE_REQUEST_ID
    ,vfio.FIO4SEARCH AS FIO
    ,vfio.FIO_AKT
    ,vfio.FIO_CREATED
    ,vfio.MODIFICATION_DATE AS FIO_MOD
    ,vb.BIRTH
    ,vb.BIRTH_AKT
    ,vb.BIRTH_CREATED
    ,vb.MODIFICATION_DATE AS BIRTH_MOD
    ,ph_mob.OBJECTS_TYPE
    ,ph_mob.PHONE AS PHONE_MOB
    ,ph_mob.PHONES_AKT
    ,ph_mob.PHONES_CREATED
    ,ph_mob.MODIFICATION_DATE AS PHONES_MOD
  FROM CPD.PHONES ph_mob, KREDIT.C_REQUEST cr, AFS.VIEW_FIO_HISTORY vfio, AFS.VIEW_BIRTH vb
  WHERE vfio.FIO_OBJECTS_ID = cr.OBJECTS_ID AND vfio.FIO_CREATED < cr.CREATED_DATE+10
      AND vb.OBJECTS_ID = cr.OBJECTS_ID AND cr.CREATED_DATE+10>vb.BIRTH_CREATED
      AND ph_mob.OBJECTS_ID=cr.OBJECTS_ID AND ph_mob.OBJECTS_TYPE=2 AND cr.CREATED_DATE+10>ph_mob.phones_created;

   COMMENT ON TABLE "AFS"."VR_MOB_FB"  IS 'Вьюха с привязкой к заявке Мобильного телефона + ФИО + ДР';
--------------------------------------------------------
--  DDL for View VR_PMG
--------------------------------------------------------

  CREATE OR REPLACE VIEW "AFS"."VR_PMG" ("REQUEST_ID", "OBJECTS_ID", "CREATED_DATE", "MODIFICATION_DATE", "CREATED_GROUP_ID", "STATUS_ID", "SCORE_TREE_ROUTE_ID", "TYPE_REQUEST_ID", "DOCUMENTS_SERIAL", "DOCUMENTS_NUMBER", "DOCUMENTS_AKT", "DOCUMENTS_TYPE", "DOCUMENTS_DATE", "DOCUMENTS_CREATED", "DOCUMENTS_MOD", "ADDRESS_HISTORY_ID", "OBJECTS_TYPE", "ADDRESS_AKT", "ADDRESS_CREATED", "ADDRESS_MOD_PMG", "ADDRESS_STR", "REGIONS_NAMES", "AREAS_NAMES", "CITIES_NAMES", "STREETS_NAMES", "HOUSE", "BUILD", "FLAT", "ADDRESS_ID") AS 
  SELECT cr.REQUEST_ID
    ,cr.OBJECTS_ID
    ,cr.CREATED_DATE
    ,cr.MODIFICATION_DATE
    ,cr.CREATED_GROUP_ID
,cr.STATUS_ID
,cr.SCORE_TREE_ROUTE_ID
,cr.TYPE_REQUEST_ID
    ,vdh.DOCUMENTS_SERIAL
    ,vdh.DOCUMENTS_NUMBER
    ,vdh.DOCUMENTS_AKT
    ,vdh.DOCUMENTS_TYPE
    ,vdh.DOCUMENTS_DATE
    ,vdh.DOCUMENTS_CREATED
    ,vdh.MODIFICATION_DATE AS DOCUMENTS_MOD
    ,vah_PMG.ADDRESS_HISTORY_ID
    ,vah_PMG.OBJECTS_TYPE
    ,vah_PMG.ADDRESS_AKT
    ,vah_PMG.ADDRESS_CREATED
    ,vah_PMG.MODIFICATION_DATE AS ADDRESS_MOD_PMG
    ,vah_PMG.ADDRESS_STR
    ,vah_PMG.regions_names 
    ,vah_PMG.areas_names
    ,vah_PMG.cities_names 
    ,vah_PMG.streets_names
    ,vah_PMG.house
    ,vah_PMG.build 
    ,vah_PMG.flat
    ,vah_PMG.ADDRESS_ID
  FROM KREDIT.C_REQUEST cr
  INNER JOIN AFS.VIEW_DOCUMENTS_HISTORY vdh
    ON cr.OBJECTS_ID=vdh.OBJECTS_ID AND vdh.DOCUMENTS_TYPE=21 AND cr.CREATED_DATE+10>vdh.DOCUMENTS_CREATED
       AND cr.CREATED_DATE+10>TO_DATE(vdh.DOCUMENTS_DATE, 'dd.mm.yyyy')
  INNER JOIN AFS.VIEW_ADDRESS_HISTORY vah_PMG --присоединяем адрес прописки
    ON vah_PMG.OBJECTS_ID=vdh.DOC_OBJECTS_ID AND vah_PMG.OBJECTS_TYPE=5 AND cr.CREATED_DATE+10>vah_PMG.address_created;

   COMMENT ON TABLE "AFS"."VR_PMG"  IS 'Вьюха с привязкой к заявке данных по ПМЖ';
