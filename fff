  CREATE OR REPLACE VIEW "AFS"."VR_FMG" ("REQUEST_ID", "OBJECTS_ID", "CREATED_DATE", "MODIFICATION_DATE", "CREATED_GROUP_ID", "STATUS_ID", "SCORE_TREE_ROUTE_ID", "TYPE_REQUEST_ID", "ADDRESS_HISTORY_ID", "OBJECTS_TYPE", "ADDRESS_AKT", "ADDRESS_CREATED", "ADDRESS_MOD", "ADDRESS_STR", "REGIONS_NAMES", "AREAS_NAMES", "CITIES_NAMES", "STREETS_NAMES", "HOUSE", "BUILD", "FLAT", "ADDRESS_ID") AS 
  SELECT cr.REQUEST_ID
    ,cr.OBJECTS_ID
    ,cr.CREATED_DATE
    ,cr.MODIFICATION_DATE
    ,cr.CREATED_GROUP_ID
	,cr.STATUS_ID
	,cr.SCORE_TREE_ROUTE_ID
	,cr.TYPE_REQUEST_ID
    ,vah_FMG.ADDRESS_HISTORY_ID
    ,vah_FMG.OBJECTS_TYPE
    ,vah_FMG.ADDRESS_AKT
    ,vah_FMG.ADDRESS_CREATED
    ,vah_FMG.MODIFICATION_DATE AS ADDRESS_MOD
    ,vah_FMG.ADDRESS_STR 
    ,vah_FMG.regions_names
    ,vah_FMG.areas_names
    ,vah_FMG.cities_names
    ,vah_FMG.streets_names
    ,vah_FMG.house 
    ,vah_FMG.build
    ,vah_FMG.flat
    ,vah_FMG.ADDRESS_ID
  FROM KREDIT.C_REQUEST cr
  INNER JOIN AFS.VIEW_ADDRESS_HISTORY vah_FMG
    ON vah_FMG.OBJECTS_ID=cr.OBJECTS_ID AND vah_FMG.OBJECTS_TYPE=2 AND cr.CREATED_DATE+10>vah_FMG.address_created;

   COMMENT ON TABLE "AFS"."VR_FMG"  IS 'Вьюха с привязкой к заявке данных по ФМЖ';
