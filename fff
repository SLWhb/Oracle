  END FRAUD_0008;


END AFS_RULES_PASP;

/
--------------------------------------------------------
--  DDL for Package Body AFS_RULES_PHONE
--------------------------------------------------------

  CREATE OR REPLACE PACKAGE BODY "AFS"."AFS_RULES_PHONE" AS

  PROCEDURE FRAUD_0001(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2 
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      --Правило 3 -	Телефон1-Псп0
      p_FRAUD_ID			NUMBER := 3;   --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК

      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := '';
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT * FROM (     
              SELECT DISTINCT v_src.REQUEST_ID, v_src.OBJECTS_ID AS PERSON_ID, v_src.CREATED_DATE AS REQUEST_DATE
                  , v_rel.REQUEST_ID AS REQUEST_ID_REL, v_rel.OBJECTS_ID AS PERSON_ID_REL
                  , v_rel.CREATED_DATE AS REQUEST_DATE_REL
                  , ABS(ROUND(v_src.CREATED_DATE-v_rel.CREATED_DATE, 2)) as DAY_BETWEEN
                  , 'Тел.моб:'||v_src.PHONE_MOB as INFO_EQUAL
                  ,'Фио+Др.:'||v_src.FIO||' '||v_src.BIRTH as INFO_NOT_EQUAL
                  ,DENSE_RANK() OVER (PARTITION BY v_src.OBJECTS_ID 
                              ORDER BY v_src.fio_akt DESC,  v_src.FIO_CREATED DESC, v_src.FIO_MOD DESC
                                ,v_src.BIRTH_AKT DESC, v_src.BIRTH_CREATED DESC, v_src.BIRTH_MOD DESC) as RANK
                  ,'Фио+Др.:'||v_rel.FIO||' '||v_rel.BIRTH as INFO_NOT_EQUAL_REL
                  ,DENSE_RANK() OVER (PARTITION BY v_rel.OBJECTS_ID 
                              ORDER BY v_rel.fio_akt DESC,  v_rel.FIO_CREATED DESC, v_rel.FIO_MOD DESC
                                ,v_rel.BIRTH_AKT DESC, v_rel.BIRTH_CREATED DESC, v_rel.BIRTH_MOD DESC) as RANK_REL
                  ,DENSE_RANK() OVER (PARTITION BY v_src.REQUEST_ID, doc_src.OBJECTS_ID 
                              ORDER BY doc_src.documents_akt DESC,  doc_src.documents_CREATED DESC
                               , doc_src.documents_CREATED DESC, doc_src.DOCUMENTS_HISTORY_ID desc) as RANK_DOC_SRC
                  ,DENSE_RANK() OVER (PARTITION BY v_src.REQUEST_ID, doc_rel.OBJECTS_ID 
                              ORDER BY doc_rel.documents_akt DESC,  doc_rel.documents_CREATED DESC
                               , doc_rel.documents_CREATED DESC, doc_rel.DOCUMENTS_HISTORY_ID desc) as RANK_DOC_REL
                  ,doc_src.DOCUMENTS_SERIAL as DOCUMENTS_SERIAL_SRC
                  ,doc_src.DOCUMENTS_NUMBER as DOCUMENTS_NUMBER_SRC
                  ,doc_rel.DOCUMENTS_SERIAL as DOCUMENTS_SERIAL_REL
                  ,doc_rel.DOCUMENTS_NUMBER as DOCUMENTS_NUMBER_REL
                  ,v_src.FIO AS FIO, v_src.BIRTH AS DR
                  ,v_rel.FIO AS FIO_REL, v_rel.BIRTH AS DR_REL
                  ,1 AS FRAUD_OUTNUMBER
                FROM AFS.VR_MOB_FB v_src
                INNER JOIN AFS.VR_MOB_FB v_rel
                  ON v_src.PHONE_MOB = v_rel.PHONE_MOB
                    AND v_src.OBJECTS_TYPE = v_rel.OBJECTS_TYPE
                    AND v_src.OBJECTS_ID ^= v_rel.OBJECTS_ID
                  AND v_src.REQUEST_ID > v_rel.REQUEST_ID
                INNER JOIN AFS.VR_DOC doc_src 
                  ON doc_src.REQUEST_ID = v_src.REQUEST_ID and doc_src.DOCUMENTS_AKT=1
                    AND NOT doc_src.DOCUMENTS_SERIAL='-' AND NOT doc_src.DOCUMENTS_SERIAL='-'
                    AND NOT doc_src.DOCUMENTS_SERIAL IS NULL AND NOT doc_src.DOCUMENTS_SERIAL IS NULL
                INNER JOIN AFS.VR_DOC doc_rel
                  ON doc_rel.REQUEST_ID = v_rel.REQUEST_ID and doc_rel.DOCUMENTS_AKT=1
                WHERE v_src.REQUEST_ID = v_Request_ID AND v_src.OBJECTS_TYPE=2
                  AND (v_src.FIO^=v_rel.FIO OR v_src.BIRTH^=v_rel.BIRTH)
                  AND v_rel.CREATED_DATE BETWEEN v_src.CREATED_DATE-91*1 AND v_src.CREATED_DATE
                  AND NOT v_src.PHONE_MOB LIKE '%000000%' AND NOT v_src.PHONE_MOB LIKE '%999999%'
               ) WHERE RANK=1 AND RANK_REL=1 
                  and RANK_DOC_SRC=1 and RANK_DOC_REL=1
                  and DOCUMENTS_SERIAL_REL || DOCUMENTS_NUMBER_REL <> DOCUMENTS_SERIAL_SRC || DOCUMENTS_NUMBER_SRC
              --оставляем только заявки не попавшие под общий фильтр
              AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
              --исключаем девичьи фамилии
              AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL )
              -- исключение однофамильцев 
              AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
               --PostCheck: Модифицированная проверка на родственников
              AND NOT AFS.FN_IS_FAMILY_REL(PERSON_ID, PERSON_ID_REL)=1
              AND NOT AFS.FN_IS_FAMILY_REL(PERSON_ID_REL, PERSON_ID)=1
              AND NOT AFS.FN_IS_FAMILY_CONT(PERSON_ID, PERSON_ID_REL)=1 
              AND NOT AFS.FN_IS_FAMILY_CONT(PERSON_ID_REL, PERSON_ID)=1
              AND NOT EXISTS(SELECT 1 FROM CPD.PHONES 
                                WHERE (OBJECTS_ID=PERSON_ID OR OBJECTS_ID=PERSON_ID_REL ) AND OBJECTS_TYPE=2 
                                    AND PHONE=SUBSTR(INFO_EQUAL, 9, 10)
                                    AND PHONES_COMM = 'Телефон из РБО: Мобильный') )
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0001;

  PROCEDURE FRAUD_0002(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      p_MIN_CNT_DAY NUMBER := 0;    --минимальное число дней для в периоде
      p_MAX_CNT_DAY NUMBER := 1;    --макесимальное число дней в периоде

      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
        CASE 
          WHEN p_FRAUD_ID=46 THEN 
            p_MIN_CNT_DAY := 0; p_MAX_CNT_DAY := 1-1/86400;
          WHEN p_FRAUD_ID=47 THEN 
            p_MIN_CNT_DAY := 1; p_MAX_CNT_DAY := 2-1/86400;
          WHEN p_FRAUD_ID=48 THEN 
            p_MIN_CNT_DAY := 2; p_MAX_CNT_DAY := 7-1/86400;
          WHEN p_FRAUD_ID=49 THEN 
            p_MIN_CNT_DAY := 7; p_MAX_CNT_DAY := 15-1/86400;
          WHEN p_FRAUD_ID=50 THEN 
            p_MIN_CNT_DAY := 15; p_MAX_CNT_DAY := 31-1/86400;
          WHEN p_FRAUD_ID=51 THEN 
            p_MIN_CNT_DAY := 31; p_MAX_CNT_DAY := 61-1/86400;
          WHEN p_FRAUD_ID=52 THEN 
            p_MIN_CNT_DAY := 61; p_MAX_CNT_DAY := 91-1/86400;
        END CASE;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT * FROM (select distinct
                v_src.request_id
                ,v_src.objects_id as person_id
                ,v_src.created_date as request_date
                ,v_rel.request_id as request_id_REL
                ,v_rel.objects_id as person_id_REL
                ,v_rel.created_date as request_date_REL
                ,round(v_src.created_date - v_rel.created_date,2) as day_between
                ,'Тел.моб:' || ph.PHONE as info_equal
                ,v_src.FIO ||' '|| v_src.BIRTH || ' Пасп: '||v_src.DOCUMENTS_SERIAL||' '|| v_src.DOCUMENTS_NUMBER as info_not_equal
                ,1 as rank
                ,v_rel.FIO  ||' '|| v_rel.BIRTH || ' Пасп: '|| v_rel.DOCUMENTS_SERIAL||' ' || v_rel.DOCUMENTS_NUMBER as info_not_equal_rel
                ,1 as rank_rel
                ,v_src.FIO as fio, v_src.BIRTH as dr
                ,v_rel.FIO as fio_rel, v_rel.BIRTH as dr_rel
                ,1 as fraud_outnumber
              from AFS.VR_DOC_FB v_src
              inner join CPD.PHONES ph 
                on ph.objects_id = v_src.objects_id and ph.objects_type = 2 and ph.PHONES_AKT=1
                  and trunc(ph.phones_created) <= trunc(CASE when v_src.created_date<SYSDATE-62 then v_src.created_date else SYSDATE end)
              inner join CPD.PHONES ph_rel --ищем такой же личн. телефон
                on ph_rel.PHONE = ph.PHONE and ph_rel.objects_type = 2 and ph_rel.PHONES_AKT=1
                  and trunc(ph_rel.phones_created) <= trunc(CASE when v_src.created_date<SYSDATE-62 then v_src.created_date else SYSDATE end)
                  and ph.objects_id <> ph_rel.objects_id 
                  --and ph.PHONES <> '0000000'
                  AND NOT ph.PHONE LIKE '%000000%' AND NOT ph.PHONE LIKE '%999999%'
              inner join AFS.VR_DOC_FB v_rel --привязываем доп. данные по связанному телефону/физику
                on ph_rel.objects_id = v_rel.objects_id and v_rel.type_request_id = 1
                  and v_rel.created_date < (CASE when v_src.created_date<SYSDATE-62 then v_src.created_date else SYSDATE end)
                  --- в теч. дня
                  and (CASE when v_src.created_date<SYSDATE-62 then v_src.created_date else SYSDATE end) 
                    - v_rel.created_date BETWEEN p_MIN_CNT_DAY AND p_MAX_CNT_DAY
                  --доп. условие, которое должно различаться
                  and v_src.FIO || v_src.BIRTH <> v_rel.FIO|| v_rel.BIRTH
                  and v_rel.DOCUMENTS_SERIAL || v_rel.DOCUMENTS_NUMBER <> v_src.DOCUMENTS_SERIAL || v_src.DOCUMENTS_NUMBER
              where v_src.request_id = v_Request_ID --входящая заявка 
                and v_src.FIO_AKT =1 and v_src.BIRTH_AKT = 1 and v_src.DOCUMENTS_AKT = 1
                and v_rel.FIO_AKT =1 and v_rel.BIRTH_AKT = 1 and v_rel.DOCUMENTS_AKT = 1
                --переделанная фильтрация на привязываемую заявку. 
                and v_rel.TYPE_REQUEST_ID=1 and v_rel.CREATED_GROUP_ID NOT IN(4862,4863,4865,4864,5736,5899,7674,11455,10165)
                and NOT v_rel.score_tree_route_id IN(
                      1	--Мгновенные кросс-продажи
                      ,4	--Для тестирования интерфейса
                      ,6	--предпринятое решение
                      ,7	--Дистанционное Увеличение лимита КК
                      ,9	--Изменение существенных условий по ТП «КК новогодний кредит»
                      ,10	--Увеличение лимита по ТП «КК новогодний кредит»
                      ,14	--Повышение ставки по договору сотрудника
                      ,16	--Изменение существенных условий по ТП «КК Аппетитный кредит»
                      ,17	--Увеличение лимита по ТП «КК Аппетитный кредит»
                      ,18	--Выдача кредитов МФО
                      ,20	--Изменение лимита по ВИП КК
                      ,35	--Мгновенные КК вкладчикам (Instant)
                      )
            ) WHERE RANK=1 AND RANK_REL=1
              --оставляем только заявки не попавшие под общий фильтр
              --AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
              AND NOT AFS.FN_IS_FAMILY_REL(PERSON_ID, PERSON_ID_REL)=1
              AND NOT AFS.FN_IS_FAMILY_REL(PERSON_ID_REL, PERSON_ID)=1
              AND NOT AFS.FN_IS_FAMILY_CONT(PERSON_ID, PERSON_ID_REL)=1 
              AND NOT AFS.FN_IS_FAMILY_CONT(PERSON_ID_REL, PERSON_ID)=1
              AND NOT EXISTS(SELECT 1 FROM CPD.PHONES 
                                WHERE (OBJECTS_ID=PERSON_ID OR OBJECTS_ID=PERSON_ID_REL ) AND OBJECTS_TYPE=2 
                                    AND PHONE=SUBSTR(INFO_EQUAL, 9, 10)
                                    AND PHONES_COMM = 'Телефон из РБО: Мобильный') )
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0002;

  PROCEDURE FRAUD_0003(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (-- afs_rules_phone.fraud_0003 - 'Яма_5'
                SELECT DISTINCT * FROM 
                                (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                          ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                          ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                          --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                          ,'Тел.моб:'||src.PHONE_MOB as INFO_EQUAL
                          ,'Адр.прж:'||fmg_src.regions_names||', '||fmg_src.areas_names||', '||fmg_src.cities_names||', '||fmg_src.streets_names
                            ||', '||fmg_src.house||', '||fmg_src.build||', '||fmg_src.flat as INFO_NOT_EQUAL
                          ,'Адр.прж:'||fmg_rel.regions_names||', '||fmg_rel.areas_names||', '||fmg_rel.cities_names||', '||fmg_rel.streets_names
                            ||', '||fmg_rel.house||', '||fmg_rel.build||', '||fmg_rel.flat  as INFO_NOT_EQUAL_REL
                          ,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                                ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK
                          ,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.REQUEST_ID
                                ORDER BY fmg_src.ADDRESS_AKT desc, fmg_src.ADDRESS_CREATED desc
                                  , fmg_rel.ADDRESS_AKT DESC, fmg_rel.ADDRESS_CREATED DESC) as RNK_ADR
                          ,SRC.FIO as FIO, SRC.BIRTH as DR
                          ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                          ,1 AS FRAUD_OUTNUMBER
                        FROM AFS.VR_MOB_FB src
                        INNER JOIN AFS.VR_MOB_FB rel --таблица поиска связей
                          ON --RULE: определяем фрод-правило поиска 
                          rel.PHONE_MOB = src.PHONE_MOB
                          AND src.OBJECTS_TYPE=2 and src.PHONES_AKT=1 --and rel.PHONES_AKT=1
                          AND rel.CREATED_DATE BETWEEN src.CREATED_DATE-91*1 AND src.CREATED_DATE
                          AND rel.REQUEST_ID < src.REQUEST_ID
                          AND NOT src.PHONE_MOB LIKE '%000000%' AND NOT rel.PHONE_MOB LIKE '%000000%'
                          AND NOT src.PHONE_MOB LIKE '%999999%' AND NOT rel.PHONE_MOB LIKE '%999999%'
                          AND NOT src.PHONE_MOB='-' AND NOT rel.PHONE_MOB='-'
                          AND NOT src.PHONE_MOB IS NULL AND NOT rel.PHONE_MOB IS NULL
                        INNER JOIN AFS.VR_FMG fmg_src
                          ON src.REQUEST_ID=fmg_src.REQUEST_ID and fmg_src.ADDRESS_AKT=1
                            AND src.CREATED_DATE+1>fmg_src.ADDRESS_CREATED
                        INNER JOIN AFS.VR_FMG fmg_rel
                          ON rel.REQUEST_ID=fmg_rel.REQUEST_ID --AND fmg_rel.ADDRESS_AKT=1
                            AND rel.CREATED_DATE+1>fmg_rel.ADDRESS_CREATED
                            AND NOT (fmg_rel.regions_names=fmg_src.regions_names
                                  AND fmg_rel.areas_names=fmg_src.areas_names
                                  AND fmg_rel.cities_names=fmg_src.cities_names
                                  AND fmg_rel.streets_names=fmg_src.streets_names
                                  AND fmg_rel.house=fmg_src.house
                                  AND fmg_rel.build=COALESCE(fmg_src.build,' ')
                                  AND fmg_rel.flat=COALESCE(fmg_src.flat,' ') )
                            AND NOT fmg_src.house=' ' AND NOT fmg_src.house IS NULL
                            AND NOT fmg_src.regions_names='-' AND NOT (fmg_src.cities_names='-' and fmg_src.streets_names='-')
                      AND NOT fmg_rel.regions_names='-' AND NOT (fmg_rel.cities_names='-' and fmg_rel.streets_names='-')
                            AND NOT fmg_src.flat='-' AND NOT fmg_src.flat=' ' AND NOT fmg_src.flat IS NULL
                            AND NOT fmg_rel.flat='-' AND NOT fmg_rel.flat=' ' AND NOT fmg_rel.flat IS NULL
                        WHERE src.REQUEST_ID = v_Request_ID
                           -- between 63649970 and 63649970+100
                           ) 
                           TAB WHERE TAB.RNK=1 and RNK_ADR=1
                  --оставляем только заявки не попавшие под общий фильтр
                  AND AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
              )
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0003;

  PROCEDURE FRAUD_0004(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (-- afs_rules_phone.fraud_0004 - 'Яма_6'
                  SELECT DISTINCT * FROM 
                                  (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                            ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                            ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                            --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                            ,'Тел.моб:'||src.PHONE_MOB as INFO_EQUAL
                            ,'Тел.прж:'||fmg_src.PHONE_FMG as INFO_NOT_EQUAL
                            ,'Тел.прж:'||fmg_rel.PHONE_FMG  as INFO_NOT_EQUAL_REL
                            /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                                  ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK
                            ,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.REQUEST_ID
                                  ORDER BY fmg_src.ADDRESS_AKT desc, fmg_src.ADDRESS_CREATED desc
                                    , fmg_rel.ADDRESS_AKT DESC, fmg_rel.ADDRESS_CREATED DESC) as RNK_ADR*/
                            ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                                ORDER BY src.CREATED_DATE DESC, fmg_src.ADDRESS_AKT DESC, COALESCE(fmg_src.ADDRESS_MOD, fmg_src.ADDRESS_CREATED) DESC
                                  , fmg_src.PHONES_AKT DESC, COALESCE(fmg_src.PHONES_MOD, fmg_src.PHONES_CREATED) DESC
                                    ) as RNK
                            ,ROW_NUMBER() OVER(PARTITION BY rel.REQUEST_ID, fmg_rel.REQUEST_ID
                                ORDER BY fmg_rel.CREATED_DATE DESC, fmg_rel.ADDRESS_AKT DESC, COALESCE(fmg_rel.ADDRESS_MOD, fmg_rel.ADDRESS_CREATED) DESC
                                  , fmg_rel.PHONES_AKT DESC, COALESCE(fmg_rel.PHONES_MOD, fmg_rel.PHONES_CREATED) DESC
                                    ) as RNK_REL
                            ,SRC.FIO as FIO, SRC.BIRTH as DR
                            ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                            ,1 AS FRAUD_OUTNUMBER
                          FROM AFS.VR_MOB_FB src
                          INNER JOIN AFS.VR_MOB_FB rel --таблица поиска связей
                            ON --RULE: определяем фрод-правило поиска 
                            rel.PHONE_MOB = src.PHONE_MOB and rel.OBJECTS_ID=src.OBJECTS_ID
                            AND src.OBJECTS_TYPE=2 and src.PHONES_AKT=1 and rel.PHONES_AKT=1
                            AND rel.CREATED_DATE BETWEEN src.CREATED_DATE-91*1 AND src.CREATED_DATE
                            AND rel.REQUEST_ID < src.REQUEST_ID
                            AND NOT src.PHONE_MOB LIKE '%000000%' AND NOT rel.PHONE_MOB LIKE '%000000%'
                            AND NOT src.PHONE_MOB LIKE '%999999%' AND NOT rel.PHONE_MOB LIKE '%999999%'
                            AND NOT src.PHONE_MOB='-' AND NOT rel.PHONE_MOB='-'
                            AND NOT src.PHONE_MOB IS NULL AND NOT rel.PHONE_MOB IS NULL
                          INNER JOIN AFS.VR_FMG_PH fmg_src
                            ON src.REQUEST_ID=fmg_src.REQUEST_ID
                              and fmg_src.ADDRESS_AKT=1
                              AND src.CREATED_DATE+1>fmg_src.ADDRESS_CREATED
                        and fmg_src.PHONES_AKT=1
                          INNER JOIN AFS.VR_FMG_PH fmg_rel
                            ON rel.REQUEST_ID=fmg_rel.REQUEST_ID
                              --AND fmg_rel.ADDRESS_AKT=1
                              AND rel.CREATED_DATE+1>fmg_rel.ADDRESS_CREATED
                              AND NOT fmg_rel.PHONE_FMG=fmg_src.PHONE_FMG
                        AND NOT fmg_src.PHONE_FMG LIKE '%000000%' AND NOT fmg_rel.PHONE_FMG LIKE '%000000%'
                        AND NOT fmg_src.PHONE_FMG LIKE '%999999%' AND NOT fmg_rel.PHONE_FMG LIKE '%999999%'
                        AND NOT fmg_src.PHONE_FMG=' ' AND NOT fmg_rel.PHONE_FMG=' '
                        AND NOT fmg_src.PHONE_FMG IS NULL AND NOT fmg_rel.PHONE_FMG IS NULL
                        AND NOT fmg_src.PHONE_FMG='-' AND NOT fmg_rel.PHONE_FMG='-'
                          WHERE src.REQUEST_ID = v_Request_ID --IN (55602874, 55596961,55589607,55578924,55524988,55487051,55486329)
                              --between 63649970 and 63649970+10
                             ) 
                             TAB WHERE RNK=1 and RNK_REL=1
                            --оставляем только заявки не попавшие под общий фильтр
                            and AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
                             --ни одна из заявок не должна быть ПОС
                            and NOT AFS.FN_IS_POS_RID(REQUEST_ID)=1 AND NOT AFS.FN_IS_POS_RID(REQUEST_ID_REL)=1)
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0004;

  PROCEDURE FRAUD_0005(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (-- afs_rules_phone.fraud_0005 - 'Яма_7'
          SELECT DISTINCT * FROM 
                          (SELECT DISTINCT SRC.REQUEST_ID,SRC.OBJECTS_ID as PERSON_ID, SRC.CREATED_DATE AS REQUEST_DATE
                    ,REL.REQUEST_ID as REQUEST_ID_REL, REL.OBJECTS_ID as PERSON_ID_REL, REL.CREATED_DATE AS REQUEST_DATE_REL
                    ,ABS(ROUND(SRC.CREATED_DATE-REL.CREATED_DATE, 0)) as DAY_BETWEEN
                    --INFO: для каждого правила переобозначить инфу в колонках INFO_EQ, INFO_NEQ, INFO_NEQ_REL
                    ,'Тел.прж:'||src.PHONE_FMG as INFO_EQUAL
                    ,'Тел.моб:'||mob_src.PHONE_MOB as INFO_NOT_EQUAL
                    ,'Тел.моб:'||mob_rel.PHONE_MOB  as INFO_NOT_EQUAL_REL
                    /*,ROW_NUMBER() OVER(PARTITION BY SRC.REQUEST_ID,REL.OBJECTS_ID 
                          ORDER BY SRC.REQUEST_ID,REL.OBJECTS_ID DESC, REL.CREATED_DATE DESC) as RNK
                    */
                    ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                      ORDER BY src.CREATED_DATE DESC, src.ADDRESS_AKT DESC, COALESCE(src.ADDRESS_MOD, src.ADDRESS_CREATED) DESC
                      , src.PHONES_AKT DESC, COALESCE(src.PHONES_MOD, src.PHONES_CREATED) DESC ) as RNK
                    ,ROW_NUMBER() OVER(PARTITION BY src.REQUEST_ID,src.OBJECTS_ID 
                      ORDER BY rel.CREATED_DATE DESC, rel.ADDRESS_AKT DESC, COALESCE(rel.ADDRESS_MOD, rel.ADDRESS_CREATED) DESC
                      , rel.PHONES_AKT DESC, COALESCE(rel.PHONES_MOD, rel.PHONES_CREATED) DESC ) as RNK_REL
                    ,SRC.FIO as FIO, SRC.BIRTH as DR
                    ,REL.FIO as FIO_REL, REL.BIRTH as DR_REL
                    ,1 AS FRAUD_OUTNUMBER
                  FROM AFS.VR_FMG_FB_PH src
                  INNER JOIN AFS.VR_FMG_FB_PH rel --таблица поиска связей
                    ON --RULE: определяем фрод-правило поиска 
                    rel.PHONE_FMG = src.PHONE_FMG
                    --AND src.ADDRESS_AKT=1 --AND rel.ADDRESS_AKT=1
                    AND src.OBJECTS_TYPE=2 and src.PHONES_AKT=1 --and rel.PHONES_AKT=1
                    AND rel.CREATED_DATE BETWEEN src.CREATED_DATE-91*1 AND src.CREATED_DATE
                    /*AND rel.REQUEST_ID < src.REQUEST_ID
                    AND NOT src.PHONE_FMG LIKE '%000000%' AND NOT rel.PHONE_FMG LIKE '%000000%'
                    AND NOT src.PHONE_FMG LIKE '%999999%' AND NOT rel.PHONE_FMG LIKE '%999999%'
                    AND NOT src.PHONE_FMG='-' AND NOT rel.PHONE_FMG='-'
                    AND NOT src.PHONE_FMG IS NULL AND NOT rel.PHONE_FMG IS NULL*/
                  INNER JOIN AFS.VR_MOB mob_src
                    ON src.REQUEST_ID=mob_src.REQUEST_ID
                      AND src.CREATED_DATE+1>mob_src.PHONES_CREATED
                and mob_src.PHONES_AKT=1
                  INNER JOIN AFS.VR_MOB mob_rel
                    ON mob_rel.REQUEST_ID=rel.REQUEST_ID and NOT mob_src.OBJECTS_ID=mob_rel.OBJECTS_ID
                      --AND fmg_rel.ADDRESS_AKT=1
                      AND rel.CREATED_DATE+1>mob_rel.PHONES_CREATED
                      AND NOT mob_rel.PHONE_MOB=mob_src.PHONE_MOB
                      AND NOT mob_src.PHONE_MOB LIKE '%000000%' AND NOT mob_rel.PHONE_MOB LIKE '%000000%'
                      AND NOT mob_src.PHONE_MOB LIKE '%999999%' AND NOT mob_rel.PHONE_MOB LIKE '%999999%'
                      AND NOT mob_src.PHONE_MOB=' ' AND NOT mob_rel.PHONE_MOB=' '
                      AND NOT mob_src.PHONE_MOB IS NULL AND NOT mob_rel.PHONE_MOB IS NULL
                      AND NOT mob_src.PHONE_MOB='-' AND NOT mob_rel.PHONE_MOB='-'
                  WHERE src.REQUEST_ID = v_Request_ID --IN (64633697,64631629,64630393,64629604,64619961, 64660930)
                     -- between 60649970 and 60649970+100
                     ) 
                     TAB WHERE RNK=1 and RNK_REL=1
                --из заявок не должна быть ПОС
                and NOT AFS.FN_IS_POS_RID(REQUEST_ID)=1 AND NOT AFS.FN_IS_POS_RID(REQUEST_ID_REL)=1
                AND NOT (SUBSTR(FIO, INSTR(FIO,' '), 254 )=SUBSTR(FIO_REL, INSTR(FIO_REL,' '), 254 ) AND DR=DR_REL 
                /*AND SRC.MOBILE=APP.MOBILE*/)
                /* исключение однофамильцев */
                AND NOT UTL_MATCH.EDIT_DISTANCE(SUBSTR(FIO, 1, INSTR(FIO,' ')-1 ), SUBSTR(FIO_REL, 1, INSTR(FIO_REL,' ')-1 ))<3
                --PostCheck: Модифицированная проверка на родственников
                AND NOT AFS.FN_IS_FAMILY_REL(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID, TAB.PERSON_ID_REL)=1 
                AND NOT AFS.FN_IS_FAMILY_CONT(TAB.PERSON_ID_REL, TAB.PERSON_ID)=1
                --оставляем только заявки не попавшие под общий фильтр
                and AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
                )
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0005;

  PROCEDURE FRAUD_0006(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0006;

  PROCEDURE FRAUD_0007(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0007;

  PROCEDURE FRAUD_0008(v_PROC_ID IN OUT NUMBER 
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0008;

END AFS_RULES_PHONE;

/
--------------------------------------------------------
--  DDL for Package Body AFS_RULES_REQUEST
--------------------------------------------------------

  CREATE OR REPLACE PACKAGE BODY "AFS"."AFS_RULES_REQUEST" AS

  PROCEDURE FRAUD_0001(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК
      
      p_MIN_CNT_DAY NUMBER := 0;    --минимальное число дней для в периоде
      p_MAX_CNT_DAY NUMBER := 3;    --максимальное число дней в периоде

      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
        CASE 
          WHEN p_FRAUD_ID=79 THEN p_MIN_CNT_DAY := -10000; p_MAX_CNT_DAY := 3;
        END CASE;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT * FROM (
                  select DISTINCT req.REQUEST_ID, req.OBJECTS_ID AS PERSON_ID, req.CREATED_DATE AS REQUEST_DATE
                      , cast(null as number) AS REQUEST_ID_REL, cast(null as number) AS PERSON_ID_REL
                      , cast(null as date) AS REQUEST_DATE_REL
                      , cast(null as number) as DAY_BETWEEN
                      , trim('Стаж ФС менее 3 месяцев. ')||'OBJECTS_ID='||TO_CHAR(req.created_user_id) as INFO_EQUAL
                      ,'Дата заведения учетной записи в ПКК: '||to_char(AUT.CREATED_DATE) ||'. ФС работает: '|| to_char(trunc(req.CREATED_DATE) - trunc(aut.created_date)) ||' дня(ей)' as INFO_NOT_EQUAL
                      ,1 as RANK
                      ,'' as INFO_NOT_EQUAL_REL
                      ,1 as RANK_REL
                      ,'' AS FIO,  '' AS DR
                      ,'' AS FIO_REL, '' AS DR_REL
                      ,1 AS FRAUD_OUTNUMBER
                    from kredit.c_request req
                    inner join kredit.auth aut
                      on aut.person_id = req.created_user_id 
                        and months_between(trunc(req.CREATED_DATE), trunc(aut.created_date)) BETWEEN p_MIN_CNT_DAY and p_MAX_CNT_DAY
                    where req.REQUEST_ID = v_Request_ID
                  ) WHERE RANK=1 AND RANK_REL=1
                  --оставляем только заявки не попавшие под общий фильтр
                  --and AFS.afs_filter.check_request_id_rel(REQUEST_ID_REL)=0
                )
              --оставляем только заявки не попавшие под общий фильтр
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0001;

  PROCEDURE FRAUD_0002(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
      p_FRAUD_ID			NUMBER;        --ID правила. Определяется по справочнику D_FRAUD_RULE
      p_FlagEx 				NUMBER;        --Флаг наличия заявки в ПКК

      pragma autonomous_transaction; 
  BEGIN
      --определяем новый уникальный ID процесса
      IF v_PROC_ID IS NULL OR v_PROC_ID<0 THEN 
        SELECT AFS.AFS_PROC_SEQ.NEXTVAL INTO v_PROC_ID FROM DUAL;
      END IF; 
      
      BEGIN --периодизируем правило в зависимости от его номера
        SELECT FRAUD_ID INTO p_FRAUD_ID FROM AFS.AFS_LOG_CALL WHERE PROC_ID = v_PROC_ID and rownum=1;
      EXCEPTION WHEN OTHERS
        THEN 
          v_OutNumber := -1; 
          v_OutChar := TO_CHAR(v_OutNumber);
          --присваиваем ошибки для системного мониторинга
          v_ErrorCode := 10032;
          v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
      END;
  
      v_OutNumber := NULL;
      v_OutChar := '';
      v_OutArray := '';
      v_ErrorCode := 0;
      v_ErrorText := ''; 
      
      --проверяем существует ли заявка
      SELECT COUNT(*) INTO p_FlagEx FROM DUAL WHERE EXISTS(SELECT 1 FROM KREDIT.C_REQUEST WHERE REQUEST_ID=v_Request_ID
        AND AFS.afs_filter.check_request_id_main(v_Request_ID)=0);
      
      CASE WHEN p_FlagEx=1 THEN 
        --логика определения текущего фрод правила
        FOR ic IN (SELECT * FROM (
                     select distinct request_id, objects_id as person_id, created_date as request_date                 
                    , null request_id_rel, null as person_id_rel, null as request_date_rel
                    ,round(phones_created-react_date_max, 4) as day_between
                    ,'phone='||phone 
                        || '; phones_user_id='||TO_CHAR(phones_user_id)
                        || '; phones_group_id='||TO_CHAR(phones_group_id)
                      as info_equal 
                    ,'phone_created='||TO_CHAR(phones_created, 'dd.mm.yyyy hh24:mi:ss')
                        ||'; react_date_max='||TO_CHAR(react_date_max, 'dd.mm.yyyy hh24:mi:ss')as info_not_equal
                    ,1 as rank
                    ,'' as INFO_NOT_EQUAL_REL
                    ,1 as rank_rel
                    ,'' as fio, '' as dr, '' as fio_rel, '' as dr_rel   
                    ,1 as fraud_outnumber
                    from (select distinct cr.request_Id,cr.objects_Id
                      ,cr.created_date
                      ,cr.modification_date
                      ,p.phone
                      ,p.created_group_id as phones_group_id
                      ,p.created_user_id as phones_user_id
                      ,p.phones_created
                      --rn_ph - если в истории был неактуальный телефон, то актуальный будет иметь значение 2
                      --,dense_rank() over (partition by cr.request_Id order by p.phones_akt ) as rn_ph
                      --,rea.react_date --для тестирования используем просто дату положительной реакции (любой)
                      --На онлайне сравниваем дату макс(последней на момент запуска правила) полож реакции(см. where) 
                      ,max(rea.react_date) over (partition by cr.request_id) as react_date_max 
                    from kredit.c_request cr
                    inner join c_request_react rea 
                      on rea.request_id=cr.request_id and rea.request_new_status_id=6 and cr.status_id=14 
                        and rea.react_group_id not in (6092,8497,15640,8853,2267)
                    inner join phones p on cr.objects_id=p.objects_id and p.phones_akt=1 and p.objects_type=2
                    where cr.request_id = v_Request_ID --входящая заявка
                        and cr.type_request_id = 1 and cr.objects_type = 2
                    ) where phones_created > react_date_max
                        --and rn_ph=2 --в истории должны быть неактуальыне телефоны
                  ) WHERE RANK=1 AND RANK_REL=1
                )
              --оставляем только заявки не попавшие под общий фильтр
        LOOP
          v_OutNumber := ic.FRAUD_OUTNUMBER;
          v_OutChar := TO_CHAR(v_OutNumber);
		  
          afs_sys.write_afs_history(v_PROC_ID => v_PROC_ID
              , v_REQUEST_ID 			=> v_Request_ID
              , v_PERSON_ID 			=> ic.PERSON_ID
              , v_REQUEST_DATE 		=> ic.REQUEST_DATE
              , v_FRAUD_ID 				=> p_FRAUD_ID
              , v_FRAUD_OUTNUMBER => v_OutNumber
              , v_FIO 					  => ic.FIO 
              , v_DR 					    => ic.DR
              , v_DAY_BETWEEN 		=> ic.DAY_BETWEEN
              , v_REQUEST_ID_REL 		=> ic.REQUEST_ID_REL
              , v_PERSON_ID_REL 		=> ic.PERSON_ID_REL
              , v_REQUEST_DATE_REL 	=> ic.REQUEST_DATE_REL
              , v_FIO_REL 				  => ic.FIO_REL 
              , v_DR_REL 				    => ic.DR_REL
              , v_INFO_EQUAL 			   => ic.INFO_EQUAL
              , v_INFO_NOT_EQUAL 		 => ic.INFO_NOT_EQUAL
              , v_INFO_NOT_EQUAL_REL => ic.INFO_NOT_EQUAL_REL
              , v_E_CODE => v_ErrorCode, v_E_TEXT => v_ErrorText);
        END LOOP;
      WHEN p_FlagEx=0 THEN --если заявки нет присваиваем статус
        v_OutNumber := 9;
        v_OutChar := TO_CHAR(v_OutNumber);
      END CASE;
      
      --если фрод не нашелся возвращаем 0
      IF (v_OutNumber IS NULL) THEN 
        v_OutNumber := 0;
        v_OutChar := TO_CHAR(v_OutNumber);
      END IF;
    
      COMMIT;

    EXCEPTION WHEN OTHERS
      THEN 
        v_OutNumber := -1; 
        v_OutChar := TO_CHAR(v_OutNumber);
        --присваиваем ошибки для системного мониторинга
        v_ErrorCode := 10031;
        v_ErrorText := SUBSTR(TO_CHAR(SQLCODE)||' | '||SQLERRM,0,4000);
  END FRAUD_0002;

  PROCEDURE FRAUD_0003(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0003;

  PROCEDURE FRAUD_0004(v_PROC_ID IN OUT NUMBER
                          ,v_Request_ID IN NUMBER, v_Person_ID IN NUMBER, v_OutCode IN VARCHAR2, v_InArray IN VARCHAR2
                          ,v_OutNumber OUT NUMBER, v_OutChar OUT VARCHAR2
                          ,v_OutArray OUT VARCHAR2
                          ,v_ErrorCode OUT NUMBER, v_ErrorText OUT VARCHAR2
                        ) AS
  BEGIN
    /* TODO implementation required */
    NULL;
  END FRAUD_0004;

END AFS_RULES_REQUEST;

